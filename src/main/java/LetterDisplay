        timePeriod = AMPM;
    }
    
    public String getTimePeriod(){
        return timePeriod;
    }
    
    public String setTimePeriod(String otherTimePeriod){
        String temp;
        
        temp = timePeriod;
        timePeriod = otherTimePeriod;
        otherTimePeriod = temp;
        
        return timePeriod;
    }
    
